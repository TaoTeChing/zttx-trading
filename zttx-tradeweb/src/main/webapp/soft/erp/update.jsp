<script>
    $(function () {
        //更新日志年切换
        $(".year").click(function () {
            var logContent = $(this).parent().find('.log-content');
            var bl = logContent.is(":hidden");
            if (bl) {
                $(this).parents('.content-box').find('.log-content').hide();
                logContent.show();
                if ($(this).parent().hasClass('last')) {
                    $(this).next().show();
                } else {
                    $(this).parents('.content-box').find('.last .year-line').hide();
                }
            }
        });
    });
