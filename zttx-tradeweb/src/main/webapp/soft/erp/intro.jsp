<script>
    $("[data-animated]").hide();

    var _view = $(window).height();//可视范围高度
    $(window).resize(function(){
        //窗口变化，重新获取可视范围高度
        _view = $(window).height();
    });
    function initScroll(_scrollTop){
        $("[data-animated]").each(function(){
            var _self = $(this);
            var _top = _self.offset().top,
                    _t_st = _top - _scrollTop;
            var region =_scrollTop + _view * 1;
            if (_top < region) {
                _self.show().addClass("animated " + _self.data("animated"));
            }
            else {
                _self.hide();
                _self.removeClass("animated");
                _self.removeClass(_self.data("animated"));
            }
        });
    }
    initScroll(0);

    $(window).scroll(function(){
        var _scrollTop = $(document).scrollTop();//滚动条到顶部距离
        initScroll(_scrollTop);
    });

</script>
