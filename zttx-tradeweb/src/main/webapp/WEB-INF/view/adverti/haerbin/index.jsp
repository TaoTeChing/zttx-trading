<script src="${res}/scripts/jquery.min.js"></script>
<script src="${res}/scripts/seajs/seajs/2.1.1/sea.js"></script>
<script src="${res}/scripts/seajs_config.js"></script>
<script src="${res}/scripts/common/base-init.js"></script>
<script type="text/javascript">
    $(function () {
        seajs.use(["dialog", 'slide'], function (Dialog, Slide) {

            var slide = {},slide_dialog;

            $(".heb-page-4 .shop,.heb-page-8 a.item").click(function () {
                var json = $(this).data("pics");
                json && initDialog(json);
            });

            $(".heb-page-5 .list .item").hover(function () {
                $(this).find(".js-anima").stop().animate({ "margin-left": -196 });
            }, function () {
                $(this).find(".js-anima").stop().animate({ "margin-left": 0 });
            });
            var myslide = $("#myslide ul");
            function initDialog(json) {
                myslide.empty();
                for (var i in json) {
                    myslide.append('<li class="item"><img src="' + json[i] + '"  /></li>');
                }
                if(slide_dialog){slide_dialog.show();return false;}
                slide_dialog = new Dialog({
                    content: "#myslide",
                    width: 1040
                }).after("show", function () {
                            slide = new Slide({
                                element: '#myslide',
                                panels: "#myslide ul li.item",
                                effect: 'scrollx',autoplay:false
                            });

                        }).after("hide", function () {
                            //slide_dialog.destroy();
                            slide.destroy();
                            slide = {};
                            $(".ui-switchable-nav").empty();
                        });

                slide_dialog.show();
            }
            $(document).on("click", "#c-prev", function () {
                slide.prev();
            });
            $(document).on("click", "#c-next", function () {
                slide.next();
            });

            var enrollForm = $('#enrollForm');
            enrollForm.find("#submit").click(function () {

                var Parameter = {
                    realName: enrollForm.find("[name=realName]").val(),
                    mobile: enrollForm.find("[name=mobile]").val(),
                    businessCategory: enrollForm.find("[name=businessCategory]").val(),
                    userType: enrollForm.find("[name=userType]:checked").val(),
                    stageCode: 1
                };


                if (Parameter.realName.length > 1 && Parameter.realName.length < 5 && Parameter.mobile.length == 11 && Parameter.businessCategory.length < 33) {
                    $.post("${ctx}/tradeMeeting/activity/signup", $('#enrollForm').serialize(), function (data) {
                        if (data.code == 126000) {
                            alert("恭喜您，"+Parameter.realName+"！已报名成功。我们会在3个工作日内与您取得联系，祝您商祺！");
                        }
                        else{
                            alert(data.message);
                        }
                    }, 'json');
                }
                else {
                    alert("请认真填写报名信息");
                }
            });
        });


        var enrollTag = $(".enroll-tag").click(function () {
            $(this).animate({ left: -200 }, 382, function () {
                $(".footer").animate({ left: 0 }, 618);
            });
        });

        $(".footer .close").click(function () {
            $(".footer").animate({ left: -3000 }, 618, function () {
                enrollTag.animate({ left: 0 }, 382);
            });
        });
    });
</script>
