                        "border-color":"#a9a9a9",
                        "color":"#666"
                    });
                    $(".bread-nav .current").find(".icons-brand-up").css("background-position","-76px -13px");
                });
                /*function afterhover(){
                 if($(".breadnavdropdown").is(":hidden")){
                 console.log("111");
                 $(".bread-nav .current").css({
                 "border-color":"#a9a9a9",
                 "color":"#666"
                 });
                 $(".bread-nav .current").find(".icons-brand-up").css("background-position","-76px -13px");
