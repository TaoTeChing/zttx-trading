                 });
                 $(".bread-nav .current").find(".icons-brand-up").css("background-position","-64px -13px");
                 }
                 }*/
                seajs.use(['popup','dialog','pagination'], function (Popup,Dialog,Pagination){
                    var popup = new Popup({
                        trigger:".bread-nav .current",
