            loaded: function () {
                players.play2 = true;
            }
        }
    });

    function playandpause(name) {
        CKobject.getObjectById(name).playOrPause();
    }

    function initFlv(opt) {
        var params = {bgcolor: '#FFF', allowFullScreen: true, allowScriptAccess: 'always', wmode: 'transparent'};
        CKobject.embedSWF('${res}/video/ckplayer/ckplayer.swf', opt.selector, opt.name, '820', '560', opt.flashvars, params);
