                                clearInterval(timer);
                                playandpause("ckplayer_a1");
                            }
                        }, 1000);
                    }
                    if (toIndex === 5) {
                        timer = setInterval(function () {
                            if (players.play2 == true) {
                                clearInterval(timer);
                                playandpause("ckplayer_a2");
                            }
                        }, 1000);
                    }
                });
    });
    if (index[0] === 4) {
        timer = setInterval(function () {
            if (players.play1 == true) {
                clearInterval(timer);
