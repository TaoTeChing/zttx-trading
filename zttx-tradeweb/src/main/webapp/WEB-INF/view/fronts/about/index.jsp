            triggers: '#tabs-panel .about-tab a',
            triggerType: "click",
            panels: '#tabs-panel .index-content .about-tab-item',
            activeIndex: index[0] || 0
        }).on('switched', function (toIndex, fromIndex) {
                    if (toIndex === 4) {
                        timer = setInterval(function () {
